{
    "name": "mmidu/translatable",
    "description": "Automatic translation of json multilingual fields for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Miduri",
            "email": "mmiduri.job@gmail.com",
            "homepage": "https://github.com/mmidu"
        }
    ],
    "homepage": "https://github.com/mmidu/translatable",
    "keywords": ["Laravel", "Translatable"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Mmidu\\Translatable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mmidu\\Translatable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mmidu\\Translatable\\TranslatableServiceProvider"
            ]
        }
    }
}
